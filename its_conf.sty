% TABLE 
%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%%

\newcommand{\myTable}[2]{
	\vspace{-10pt} 
	\begin{table}[H]\small 
		\noindent
		\caption{#1}
		\vspace{5pt}
    	\centering
    	\begin{tabularx}
        	#2
    	\end{tabularx}
	\end{table}
	\normalsize\normalfont\leftskip=0cm \rightskip=0cm\vspace{5pt}
}

