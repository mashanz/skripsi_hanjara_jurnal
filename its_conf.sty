\newcommand{\image}[1]{
	\vspace{-8pt}
	\begin{figure}[H]
		\centering
		\includegraphics #1
	\end{figure}
	\vspace{-8pt}
	\normalsize\normalfont\leftskip=0cm\rightskip=0cm
}

